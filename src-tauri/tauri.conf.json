{
  "productName": "Fabian Aps",
  "version": "0.1.0",
  "identifier": "com.comboom.sucht.mcpeapsunterstrichhd",
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devUrl": "http://localhost:3000",
    "frontendDist": "../out"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "label": "main",
        "url": "/",
        "visible": false,
        "title": "Fabian Aps",
        "width": 1280,
        "height": 720
      },
      {
				"label": "splashscreen",
				"url": "/live-wallpaper?h1=mcpeaps_HD&h2=comboom.sucht"
			}
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
