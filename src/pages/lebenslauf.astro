---
import BaseLayout from "../layouts/BaseLayout.astro";
import TimeLineElement from "../components/cv/TimeLine.astro";
import Link from "../components/Link.astro";
import presentation from "../data/presentation";
---

<BaseLayout title="Lebenslauf" sideBarActiveItemID="lebenslauf">
  <style>
    .rating {
        unicode-bidi: bidi-override;
        text-align: center;
    }
  
    .rating > span {
        display: inline-block;
        position: relative;
        width: 1.1em;
    }
  
    .rating > span:before {
        content: "\2606";
        position: absolute;
    }
  </style>
  <main>
  <div class="mb-5">
    <div class="text-3xl w-full font-bold">{presentation.title}</div>
  </div>

  <div class="mb-10 text-justify">
    Ich bin ein IT-Assistent in Ausbildung.
    <br/>
    <Link
            href={`mailto:${presentation.mail}`}
            label={'E-Mail: '+ presentation.mail}
    />
    <br/>
    <Link
            href={`https://maps.apple.com/?address=Ludwig-Renn-Stra%C3%9Fe%2033,%2012679%20Berlin,%20Deutschland&ll=52.551673,13.558337`}
            label={'Adresse: Ludwig-Renn-Straße 33, 12679 Berlin, Deutschland'}
    />
    <br/>
    <Link
            href={`tel:+4917645172171`}
            label={'Telefonnummer:'+' +4917645172171'}
    />
    <br/>
    Geburtsodatum: 06.06.2003          
  </div>

  <div class="mb-5">
    <div class="text-3xl w-full font-bold">Ausbildung</div>
  </div>

  <div class="time-line-container grid gap-4 mb-10">
    <TimeLineElement
      title="Ausbildung zum IT-Assistent mit Fachabitur"
      subtitle="seit 08/2021 am Oberstufenzentrum Informations- & Medizintechnick, Berlin, Deutschland"
    />
    <TimeLineElement
      title="Grundschule + Skundarstufe I mit dem Abschluss MSA(OG)"
      subtitle="08/2010 bis 06/2021 an Cal-von-Linné-Schule, Berlin, Deutschland"
    />
  </div>

  <div class="mb-5">
    <div class="text-3xl w-full font-bold">Erfahrung</div>
  </div>

  <div class="time-line-container mb-10">
    <TimeLineElement
      title="Praktikant"
      subtitle="Von 11/2023 bis 02/2024 bei KfW Bankengruppe, Berlin, Deutschland"
    >
      Ansible Dokumentation, Ansible Playbook Automation, Anwendungsbetreung
    </TimeLineElement>

  <div class="time-line-container mb-10">
    <TimeLineElement
      title="Praktikant"
      subtitle="Von 02/2020 bis 02/2020 bei Akademie der Künste, Berlin, Deutschland"
    >
      Syntesizer entwiklung und Mastering
    </TimeLineElement>
    <TimeLineElement
    title="Praktikant"
    subtitle="Von 01/2019 bis 01/2019 bei Tosa Security & Service, Trebin, Deutschland"
  >
   Bürotätigkeiten
  </TimeLineElement>
    <TimeLineElement
    title="Praktikant"
    subtitle="Von 03/2018 bis 03/2018 bei Pfenigpfefer, Berlin, Deutschland"
  >
    Regale ein und aus gereumt
  </TimeLineElement>
  </div>

  <div class="mb-5">
    <div class="text-3xl w-full font-bold">Ehrentamtliche Tätigkeiten</div>
  </div>

  <div class="time-line-container mb-10">
    <TimeLineElement
      title="Volenteer bei Special Olympics World Games Berlin 2023"
      subtitle="Von 06/2023 bis 06/2023 bei Special Olympics Germany, Berlin, Deutschland"
    >
      Healty Athletes Program Check in/Check out System
    </TimeLineElement>
  </div>

